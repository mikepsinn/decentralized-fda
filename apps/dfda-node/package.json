{
  "name": "fda-gov-v2",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev:env": "pnpm run sb:local:start && concurrently --kill-others --names \"next,worker,cron\" --prefix-colors \"yellow,blue,magenta\" \"pnpm run dev:next\" \"pnpm run dev:worker\" \"pnpm run dev:cron\"",
    "dev": "doppler run -- pnpm run dev:next",
    "db:setup": "tsx ./scripts/setup-local-full.ts",
    "db:worker:migrate": "npx graphile-worker --schema-only",
    "check": "pnpm run type-check && pnpm run lint",
    "build": "next build",
    "dev:next": "cross-env NODE_OPTIONS='--inspect' next dev",
    "dev:worker": "dotenv -- tsx watch worker/runner.ts",
    "dev:worker:doppler": "doppler run -- tsx watch worker/runner.ts",
    "dev:cron": "tsx watch cron-enqueuer.ts",
    "dev:cron:doppler": "doppler run -- tsx watch cron-enqueuer.ts",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "generate:nav": "dotenv -- tsx ./scripts/generate-nav-tree.ts",
    "test": "vitest",
    "test:watch": "vitest --config vitest.config.ts",
    "test:ui": "vitest --ui --config vitest.config.ts",
    "test:unit": "vitest run --config vitest.config.ts tests/unit",
    "test:integration": "vitest run --config vitest.integration.config.ts tests/integration",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "sb:local:start": "npx supabase start",
    "sb:local:stop": "npx supabase stop",
    "sb:local:status": "npx supabase status",
    "sb:logs:local:tail": "docker logs -f supabase_db_fda-gov-v2",
    "db:local:reset": "npx supabase db reset --local",
    "db:local:push": "npx supabase db push --local",
    "db:local:pull": "npx supabase db pull --local",
    "db:local:diff": "npx supabase db diff --local -f migrations",
    "db:local:types": "npx supabase gen types typescript --local > lib/database.types.ts",
    "generate:constants": "tsx ./scripts/generate-constants.ts",
    "generate:schemas": "supazod -i lib/database.types.ts -o lib/database.schemas.ts -s public",
    "db:remote:migrate": "doppler run -- tsx ./scripts/remote-db-migrate.ts",
    "db:remote:reset": "doppler run -- tsx ./scripts/remote-db-reset.ts",
    "db:cloud:link": "npx supabase link --project-ref %SUPABASE_PROJECT_ID%",
    "db:cloud:push": "npx supabase db push --linked",
    "db:cloud:pull": "npx supabase db pull --linked",
    "db:cloud:diff": "npx supabase db diff --linked -f migrations",
    "db:cloud:types": "npx supabase gen types typescript --linked > lib/database.types.ts",
    "db:cloud:reset": "npx supabase db reset --linked",
    "link:cloud": "tsx ./scripts/link-supabase.ts"
  },
