# App
NEXT_PUBLIC_APP_URL=http://localhost:3000
NODE_ENV=development

# Authentication (NextAuth.js)
NEXTAUTH_URL=http://localhost:3000
# https://generate-secret.vercel.app/32
NEXTAUTH_SECRET=dummy_secret_32_chars_123456789012

# https://console.cloud.google.com/apis/credentials
# Callback URL: http://localhost:3000/api/auth/callback/google
GOOGLE_CLIENT_ID=dummy_google_client_id
GOOGLE_CLIENT_SECRET=dummy_google_client_secret

# https://resend.com/api-keys
EMAIL_SERVER=smtp://resend:dummy_api_key@smtp.resend.com:587
EMAIL_FROM=test@example.com

# https://github.com/settings/applications
# Callback URL: http://localhost:3000/api/auth/callback/github
GITHUB_CLIENT_ID=dummy_github_client_id
GITHUB_CLIENT_SECRET=dummy_github_client_secret

# Database
DATABASE_URL="postgresql://user:pass@localhost:5433/dfda_test?schema=public"

# Get this from https://platform.openai.com/account/api-keys
OPENAI_API_KEY=dummy_openai_key_1234567890

# https://aistudio.google.com/app/apikey
GOOGLE_GENERATIVE_AI_API_KEY=dummy_google_ai_key

# https://console.anthropic.com/settings/keys
ANTHROPIC_API_KEY=dummy_anthropic_key

# Redis Configuration
REDIS_URL="redis://localhost:6379"

# Get key from https://www.perplexity.ai/settings/api
PERPLEXITY_API_KEY="dummy_perplexity_key"
# Get key from https://dashboard.exa.ai/api-keys
EXASEARCH_API_KEY="dummy_exa_key"

# AWS Configuration
AWS_ACCESS_KEY_ID=test_access_key
AWS_SECRET_ACCESS_KEY=test_secret_key
AWS_REGION=us-east-1
AWS_BUCKET=test-bucket
AWS_S3_DIRECTORY=test-directory

# Tavily API Key retrieved here: https://app.tavily.com/home
TAVILY_API_KEY=dummy_tavily_key

# Redis Configuration
USE_LOCAL_REDIS=true
LOCAL_REDIS_URL=redis://localhost:6379 # or redis://redis:6379 if you're using docker compose

# Upstash Redis URL and Token retrieved here: https://console.upstash.com/redis
UPSTASH_REDIS_REST_URL=https://dummy-url.upstash.io
UPSTASH_REDIS_REST_TOKEN=dummy_upstash_token

# SearXNG Configuration
SEARXNG_API_URL=http://localhost:8080  # Replace with your local SearXNG API URL or docker http://searxng:8080
SEARXNG_DEFAULT_DEPTH=basic # Set to 'basic' or 'advanced', only affects SearXNG searches
SEARXNG_MAX_RESULTS=50 # Maximum number of results to return from SearXNG
SEARXNG_TIME_RANGE=None # Time range for search results: day, week, month, year, or None (for all time)
SEARXNG_SAFESEARCH=0 # Safe search setting: 0 (off), 1 (moderate), 2 (strict)

# DFDA Client Configuration
DFDA_CLIENT_ID=dummy_dfda_client_id
DFDA_CLIENT_SECRET=dummy_dfda_client_secret

# Resend API Key
RESEND_API_KEY=dummy_resend_key

# Keycloak Configuration
NEXT_PUBLIC_KEYCLOAK_URL=http://localhost:8080
NEXT_PUBLIC_KEYCLOAK_REALM=dfda
NEXT_PUBLIC_KEYCLOAK_CLIENT_ID=web-client